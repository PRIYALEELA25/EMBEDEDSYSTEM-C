#include "stm32 GPIO.h"



void GPIO_PeriClockControl(GPIO_RegDef_t *pGPIOx,uint8_t ENorDI){

	 if (ENorDI == ENABLE)
	    {
	        if (pGPIOx == GPIOA)
	        {
	            GPIOA_CLK_EN();
	        }
	        else if (pGPIOx == GPIOB)
	        {
	            GPIOB_CLK_EN();
	        }
	        else if (pGPIOx == GPIOC)
	        {
	            GPIOC_CLK_EN();
	        }
	        else if (pGPIOx == GPIOD)
	        {
	            GPIOD_CLK_EN();
	        }
	        else if (pGPIOx == GPIOE)
	        {
	            GPIOE_CLK_EN();
	        }
	        else if (pGPIOx == GPIOF)
	        {
	            GPIOF_CLK_EN();
	        }
	        else if (pGPIOx == GPIOG)
	        {
	            GPIOG_CLK_EN();
	        }
	        else if (pGPIOx == GPIOH)
	        {
	            GPIOH_CLK_EN();
	        }
	    }
	    else
	    {

	        if (pGPIOx == GPIOA)
	        {
	            GPIOA_CLK_DI();
	        }

	        else if (pGPIOx == GPIOB)
	        {
	            GPIOB_CLK_DI();
	        }
	        else if (pGPIOx == GPIOC)
	        {
	            GPIOC_CLK_DI();
	        }
	        else if (pGPIOx == GPIOD)
	        {
	            GPIOD_CLK_DI();
	        }
	        else if (pGPIOx == GPIOE)
	        {
	            GPIOE_CLK_DI();
	        }
	        else if (pGPIOx == GPIOF)
	        {
	            GPIOF_CLK_DI();
	        }
	        else if (pGPIOx == GPIOH)
	        {
	            GPIOH_CLK_DI();
	        }
	    }
	}




}

